<!doctype html>
<html class="<%= page_classes %>">
<head>
  <title>Diego Eis <%= ' - ' + current_article.title unless current_article.nil? %></title>
  <meta content="<%= current_page.data.description %>" name="description">

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
  <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
  <%= feed_tag :atom, "#{blog.options.prefix.to_s}/feed.xml", title: "Atom Feed" %>
  <link href='http://fonts.googleapis.com/css?family=Lato:400,300,700,900' rel='stylesheet' type='text/css'>
  <%= stylesheet_link_tag 'style' %>

  <script type="application/ld+json">
  <% if !current_page.data.sobre && current_page.path != "index.html"%>
  {
    "@context":"http://schema.org",
    "@type":"BlogPosting",
    "@id":"http://diegoeis.com<%= current_page.url %>",
    "author" : {
      "@type" : "Person",
      "@id":"http://diegoeis.com",
      "name" : "Diego Eis"
    },
    "publisher": {
      "@type":"Person",
      "name":"Diego Eis"
    },
    "image":"https://pbs.twimg.com/profile_images/570009494805946368/dfEcOkau.jpeg",
    "datePublished": "<%= current_article.date unless current_article.nil? %>",
    "dateModified": "<%= current_article.date unless current_article.nil? %>",
    "headline":"<%= current_article.title unless current_article.nil? %>",
    "articleBody":""
  }
  <% end %>

  <% if current_page.data.sobre || current_page.path == "index.html" %>
    {
      "@context": "http://www.schema.org/",
      "@type": "person",
      "@id":"http://diegoeis.com",
      "jobTitle": "Coordinator",
      "name": "Diego Eis",
      "jobTitle": "Team Coordinator",
      "birthDate": "1983.12.03",
      "gender": "male",
      "image": "http://2.gravatar.com/avatar/1bf877955dc2e43662320fd3b0280166?size=300",
      "url": "http://diegoeis.com",
          "sameAs" : [ "https://www.facebook.com/diegoeis",
          "http://twitter.com/diegoeis",
          "http://instagram.com/diegoeis"],
      "spouse": {
        "@id": "https://facebook.com/marcelaeis",
        "name":"Marcela Urzi Eis",
        "url":"https://facebook.com/marcelaeis"
      }
    }
  <% end %>
    </script>

  </script>

