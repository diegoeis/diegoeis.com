<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xml:base="http://www.exemplo.com/schemas/carros">

  <rdfs:Class rdf:ID="carros"/>

  <rdfs:Class rdf:ID="chevrolet">
    <rdfs:subClassOf rdf:resource="#carros"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="sedan">
    <rdfs:subClassOf rdf:resource="#carros"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="cruze">
    <rdfs:subClassOf rdf:resource="#sedan"/>
    <rdfs:domain rdf:resource="#chevrolet"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="pessoa"/>
  <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#integer"/>

  <rdf:Property rdf:ID="donoCarro">
    <rdfs:domain rdf:resource="#pessoa"/>
    <rdfs:domain rdf:resource="#carros"/>
  </rdf:Property>

  <rdf:Property rdf:ID="motorista">
    <rdfs:subPropertyOf rdf:resource="#donoCarro"/>
  </rdf:Property>
</rdf:RDF>